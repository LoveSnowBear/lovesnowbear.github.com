package sample.scene.upload.multiUpload2;/** * 文件上传结果的Model。<br/>  * 上传的每个文件都会对应一个UploadResult。<br/>  * 在上传结束后，该类中的信息传递给客户端。 <br/> *  * @author frank.zhang * @version 1.0 */public class UploadResult {	/**	 * 文件名	 */	private String fileName;	/**	 * 文件大小	 */	private int size;	/**	 * 服务器端存储路径	 */	private String serverStoreDir;	/**	 * 文件格式	 */	private String fileFormat;	/**	 * 两个值：done和exceedMaxFileSize。默认为done。	 */	private String status = "done";	/**	 * @return the fileName	 */	public String getFileName() {		return fileName;	}	/**	 * @param fileName	 *            the fileName to set	 */	public void setFileName(String fileName) {		this.fileName = fileName;	}	/**	 * @return the size	 */	public int getSize() {		return size;	}	/**	 * @param size	 *            the size to set	 */	public void setSize(int size) {		this.size = size;	}	/**	 * @return the serverStoreDir	 */	public String getServerStoreDir() {		return serverStoreDir;	}	/**	 * @param serverStoreDir	 *            the serverStoreDir to set	 */	public void setServerStoreDir(String serverStoreDir) {		this.serverStoreDir = serverStoreDir;	}	/**	 * @return the fileFormat	 */	public String getFileFormat() {		return fileFormat;	}	/**	 * @param fileFormat	 *            the fileFormat to set	 */	public void setFileFormat(String fileFormat) {		this.fileFormat = fileFormat;	}	/**	 * @return the status	 */	public String getStatus() {		return status;	}	/**	 * @param status	 *            the status to set	 */	public void setStatus(String status) {		this.status = status;	}}